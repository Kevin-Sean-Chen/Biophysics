    "#fix K scan N\n",
    "Ns = np.arange(10,100,5)\n",
    "R = 10\n",
    "K = 10\n",
    "mss = np.zeros((len(Ns),R))\n",
    "for ni,nn in enumerate(Ns):  #scan through number of patterns\n",
    "    for rr in range(0,mss.shape[1]):  #repeat the simulation\n",
    "        patterns, recall = Hopfield(K,nn)\n",
    "        mss[ni,rr] = ms(patterns,recall)"
